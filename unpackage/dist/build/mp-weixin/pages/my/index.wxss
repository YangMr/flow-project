.page-container{height:calc(100vh - 50px);background-color:#f4f4f4}.page-container .user-profile{height:584rpx;background-image:url(https://sl-driver.oss-cn-hangzhou.aliyuncs.com/images/profile-bg%402x.png);background-size:100% 100%;display:flex;flex-direction:column;align-items:center;color:#fff;line-height:1.8;font-size:30rpx}.page-container .user-profile .avatar{width:140rpx;height:140rpx;margin-top:90rpx;border-radius:50%;border:6rpx solid rgba(255,255,255,.4)}.page-container .user-profile .username{font-size:36rpx;margin:10rpx 0}.page-container .user-profile .no,.page-container .user-profile .mobile{font-size:28rpx}.page-container .month-overview{height:268rpx;margin:0 30rpx;border-radius:16rpx;background-color:#fff;position:relative;top:-130rpx}.page-container .month-overview .title{color:#818181;font-size:28rpx;padding-top:28rpx;text-align:center}.page-container .month-overview .title:after,.page-container .month-overview .title:before{content:"-";margin:0 10rpx}.page-container .month-overview .content{display:flex;justify-content:space-around;text-align:center;font-size:28rpx;color:#000;margin-top:40rpx}.page-container .month-overview .content .volumn{display:block;font-size:36rpx;margin-bottom:20rpx;font-weight:500}.page-container .truck-meta{margin:-100rpx 30rpx 0;border-radius:16rpx;overflow:hidden}
